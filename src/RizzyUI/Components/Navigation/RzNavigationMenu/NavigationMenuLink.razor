
@namespace RizzyUI
@inherits RzAsChildComponent<NavigationMenuLink.Slots>

@if (AsChild)
{
	@RenderAsChild()
}
else
{
	<HtmlElement Element="@EffectiveElement"
	             id="@Id"
	             class="@_slots.GetBase()"
	             @attributes="@AdditionalAttributes">
		@ChildContent
	</HtmlElement>	
}