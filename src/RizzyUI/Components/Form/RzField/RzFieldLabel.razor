
@namespace RizzyUI
@inherits RzComponent<RzFieldLabelSlots>
@typeparam TValue


@if (!string.IsNullOrEmpty(_effectiveDisplayName))
{
    <label for="@_for" @attributes="AdditionalAttributes" id="@Id" class="@SlotClasses.GetBase()">
        @_effectiveDisplayName
        @ChildContent
    </label>
}
else
{
    @ChildContent
}