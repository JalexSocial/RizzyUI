
@namespace RizzyUI
@inherits RzComponent

<HtmlElement Element="@EffectiveElement" @attributes="@AdditionalAttributes" id="@Id" class="@RootClass()" aria-label="@EffectiveAssistiveLabel">

    @if (ChildContent != null)
    {
        @ChildContent
    }
    else
    {
        @Label
    }
</HtmlElement>