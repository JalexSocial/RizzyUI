@namespace RizzyUI
@inherits RzComponent

<CascadingValue Value="this" IsFixed="true">
    <HtmlElement Element="@EffectiveElement"
                 id="@Id"
                 class="@RootClass()"
                 role="img"
                 aria-label="@EffectiveAriaLabel"
                 @attributes="@AdditionalAttributes">
        @ChildContent
    </HtmlElement>
</CascadingValue>