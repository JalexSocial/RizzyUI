
@namespace RizzyUI
@inherits RzTypographyBase

<HtmlElement Element="@Element" @attributes="@AdditionalAttributes" id="@Id" class="@RootClass()"
             x-data="rzHeading">
    @if (ChildContent == null && !string.IsNullOrEmpty(QuickReferenceTitle))
    {
        @QuickReferenceTitle
    }
    else
    {
        @ChildContent
    }
</HtmlElement>