
@namespace RizzyUI
@inherits RzComponent<RzEmbeddedPreview.Slots>

<HtmlElement Element="@EffectiveElement" @attributes="@AdditionalAttributes" class="@_slots.GetBase()">
	<iframe x-data="rzEmbeddedPreview"
	        id="@Id"
	        data-alpine-root="@Id"
	        class="@_slots.GetIFrame()"
	        srcdoc="@_content"
	        x-ref="iframe"
			title="@IFrameTitle">
	</iframe>
</HtmlElement>