
@page "/components/tabs"
@using RizzyUI

<PageTitle>Tabs</PageTitle>

<RzQuickReferenceContainer>
    <RzArticle ProseWidth="ProseWidth.UltraWide">
        <SideContent>
            <RzQuickReference />
        </SideContent>
        <MainContent>
            <RzHeading Level="HeadingLevel.H1" class="scroll-mt-20">Tabs</RzHeading>
            <RzParagraph>
                Tabs are a great way to display different sections of your website. They can be used to indicate the current section, navigate to the next or previous section, and more.
            </RzParagraph>

            <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="Default Tabs" class="scroll-mt-20">Default Tabs</RzHeading>
            <RzParagraph TextColor="SemanticColor.Foreground">
                A list of tabs with a tab panel.
            </RzParagraph>

            <RzBrowser Layout="typeof(PreviewLayout)">
                <div class="grid-cols-1 mx-auto grid min-h-[18rem] place-content-evenly place-items-center gap-4 rounded-xl p-8 xl:grid-cols-1 2xl:grid-cols-1">
                    <RzTabs>
                        <Menu>
                            <RzTabStrip>
                                <RzTab Name="groups">Groups</RzTab>
                                <RzTab Name="likes">Likes</RzTab>
                                <RzTab Name="comments">Comments</RzTab>
                                <RzTab Name="saved">Saved</RzTab>
                            </RzTabStrip>
                        </Menu>
                        <TabPanels>
                            <RzTabPanel Name="groups"><strong><u>Groups</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="likes"><strong><u>Likes</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="comments"><strong><u>Comments</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="saved"><strong><u>Saved</u></strong> tab is selected</RzTabPanel>
                        </TabPanels>
                    </RzTabs>
                </div>
            </RzBrowser>

            <RzCodeViewer Language=@CodeLanguage.Razor class="mb-10">
    &lt;RzTabs&gt;
        &lt;Menu&gt;
            &lt;RzTabStrip&gt;
                &lt;RzTab Name=&quot;groups&quot;&gt;Groups&lt;/Tab&gt;
                &lt;RzTab Name=&quot;likes&quot;&gt;Likes&lt;/Tab&gt;
                &lt;RzTab Name=&quot;comments&quot;&gt;Comments&lt;/Tab&gt;
                &lt;RzTab Name=&quot;saved&quot;&gt;Saved&lt;/Tab&gt;
            &lt;/TabStrip&gt;
        &lt;/Menu&gt;
        &lt;TabPanels&gt;
            &lt;RzTabPanel Name=&quot;groups&quot;&gt;&lt;strong&gt;&lt;u&gt;Groups&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/TabPanel&gt;
            &lt;RzTabPanel Name=&quot;likes&quot;&gt;&lt;strong&gt;&lt;u&gt;Likes&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/TabPanel&gt;
            &lt;RzTabPanel Name=&quot;comments&quot;&gt;&lt;strong&gt;&lt;u&gt;Comments&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/TabPanel&gt;
            &lt;RzTabPanel Name=&quot;saved&quot;&gt;&lt;strong&gt;&lt;u&gt;Saved&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/TabPanel&gt;
        &lt;/TabPanels&gt;
    &lt;/RzTabs&gt;
            </RzCodeViewer>
            
            <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="Tabs with icon" class="scroll-mt-20">Tabs with icon</RzHeading>
            <RzParagraph TextColor="SemanticColor.Foreground">
                A list of tabs with icons and a tab panel.
            </RzParagraph>

            <RzBrowser Layout="typeof(PreviewLayout)">
                <div class="grid-cols-1 mx-auto grid min-h-[18rem] place-content-evenly place-items-center gap-4 rounded-xl p-8 xl:grid-cols-1 2xl:grid-cols-1">
                    <RzTabs>
                        <Menu>
                            <RzTabStrip>
                                <RzTab Name="groups">
                                    <div class="pr-2"><Blazicon Svg="MdiIcon.AccountGroup" /></div>
                                    Groups
                                </RzTab>
                                <RzTab Name="likes">Likes</RzTab>
                                <RzTab Name="comments">Comments</RzTab>
                                <RzTab Name="saved">Saved</RzTab>
                            </RzTabStrip>
                        </Menu>
                        <TabPanels>
                            <RzTabPanel Name="groups"><strong><u>Groups</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="likes"><strong><u>Likes</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="comments"><strong><u>Comments</u></strong> tab is selected</RzTabPanel>
                            <RzTabPanel Name="saved"><strong><u>Saved</u></strong> tab is selected</RzTabPanel>
                        </TabPanels>
                    </RzTabs>
                </div>
            </RzBrowser>

            <RzCodeViewer Language=@CodeLanguage.Razor class="mb-10">
    &lt;RzTabs&gt;
        &lt;Menu&gt;
            &lt;RzTabStrip&gt;
                &lt;RzTab Name=&quot;groups&quot;&gt;Groups&lt;/RzTab&gt;
                &lt;RzTab Name=&quot;likes&quot;&gt;Likes&lt;/RzTab&gt;
                &lt;RzTab Name=&quot;comments&quot;&gt;Comments&lt;/RzTab&gt;
                &lt;RzTab Name=&quot;saved&quot;&gt;Saved&lt;/RzTab&gt;
            &lt;/TabStrip&gt;
        &lt;/Menu&gt;
        &lt;TabPanels&gt;
            &lt;RzTabPanel Name=&quot;groups&quot;&gt;&lt;strong&gt;&lt;u&gt;Groups&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;likes&quot;&gt;&lt;strong&gt;&lt;u&gt;Likes&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;comments&quot;&gt;&lt;strong&gt;&lt;u&gt;Comments&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;saved&quot;&gt;&lt;strong&gt;&lt;u&gt;Saved&lt;/u&gt;&lt;/strong&gt; tab is selected&lt;/RzTabPanel&gt;
        &lt;/TabPanels&gt;
    &lt;/RzTabs&gt;
            </RzCodeViewer>            

            <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="Colored Tabs" class="scroll-mt-20">Colored Tabs</RzHeading>
            <RzParagraph TextColor="SemanticColor.Foreground">
                A list of colored tabs with a tab panel.
            </RzParagraph>

            <RzBrowser Layout="typeof(PreviewLayout)">
                <div class="grid-cols-1 mx-auto grid min-h-[18rem] place-content-evenly place-items-center gap-4 rounded-xl p-8 xl:grid-cols-1 2xl:grid-cols-1">
                    <RzTabs SelectedTabName="events" 
                          TabBackgroundColor="SemanticColor.Muted"
                          SelectedTabUnderlineColor="SemanticColor.Warning"
                          TabTextColor="SemanticColor.Foreground"
                          SelectedTabTextColor="SemanticColor.Foreground">
                        <Menu>
                            <RzTabStrip Justify="Justify.Center" SpaceBetween="Size.Small">
                                <RzTab Name="activity">Activity</RzTab>
                                <RzTab Name="events">Events</RzTab>
                                <RzTab Name="pages">Pages</RzTab>
                                <RzTab Name="members">Members</RzTab>
                            </RzTabStrip>
                        </Menu>
                        <TabPanels>
                            <RzTabPanel Name="activity" class="bg-secondary dark:bg-secondary rounded-b p-4">Activity Content</RzTabPanel>
                            <RzTabPanel Name="events" class="bg-secondary dark:bg-secondary rounded-b p-4">Events Content</RzTabPanel>
                            <RzTabPanel Name="pages" class="bg-secondary dark:bg-secondary rounded-b p-4">Pages Content</RzTabPanel>
                            <RzTabPanel Name="members" class="bg-secondary dark:bg-secondary rounded-b p-4">Members Content</RzTabPanel>
                        </TabPanels>
                    </RzTabs>
                </div>
            </RzBrowser>

            <RzCodeViewer Language=@CodeLanguage.Razor class="mb-10">
    &lt;RzTabs SelectedTabName=&quot;events&quot; 
          TabBackgroundSemanticColor=&quot;SemanticColor.SurfaceAlt&quot;
          SelectedTabUnderlineSemanticColor=&quot;SemanticColor.Warning&quot;
          TabTextSemanticColor=&quot;SemanticColor.OnSurface&quot;
          SelectedTabTextSemanticColor=&quot;SemanticColor.OnSurfaceStrong&quot;&gt;
        &lt;Menu&gt;
            &lt;RzTabStrip Justify=&quot;Justify.Center&quot; SpaceBetween=&quot;Size.Medium&quot;&gt;
                &lt;RzTab Name=&quot;activity&quot;&gt;Activity&lt;/RzTab&gt;
                &lt;RzTab Name=&quot;events&quot;&gt;Events&lt;/RzTab&gt;
                &lt;RzTab Name=&quot;pages&quot;&gt;Pages&lt;/RzTab&gt;
                &lt;Rzab Name=&quot;members&quot;&gt;Members&lt;/RzTab&gt;
            &lt;/TabStrip&gt;
        &lt;/Menu&gt;
        &lt;TabPanels&gt;
            &lt;RzTabPanel Name=&quot;activity&quot; class=&quot;bg-secondary dark:bg-secondary rounded-b p-4&quot;&gt;Activity Content&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;events&quot; class=&quot;bg-secondary dark:bg-secondary rounded-b p-4&quot;&gt;Events Content&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;pages&quot; class=&quot;bg-secondary dark:bg-secondary rounded-b p-4&quot;&gt;Pages Content&lt;/RzTabPanel&gt;
            &lt;RzTabPanel Name=&quot;members&quot; class=&quot;bg-secondary dark:bg-secondary rounded-b p-4&quot;&gt;Members Content&lt;/RzTabPanel&gt;
        &lt;/TabPanels&gt;
    &lt;/Tabs&gt;
            </RzCodeViewer>

            <!-- RzTabs Component Parameters -->
            <section class="my-8 py-2">
                <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="RzTabs Parameters" class="scroll-mt-20">
                    RzTabs Component Parameters
                </RzHeading>
                  <div class="not-prose mt-6 mb-10 overflow-hidden rounded-md border border-zinc-200 dark:border-zinc-800">
                    <table class="w-full text-sm">
                        <thead class="text-left bg-zinc-100/75 font-semibold text-zinc-700 dark:bg-zinc-900/50 dark:text-zinc-300">
                            <tr>
                                <th scope="col" class="w-1/5 px-4 py-3">Property</th>
                                <th scope="col" class="w-2/5 px-4 py-3">Description</th>
                                <th scope="col" class="w-1/5 px-4 py-3">Type</th>
                                <th scope="col" class="px-4 py-3">Default</th>
                            </tr>
                        </thead>
                        <tbody class="divide-y divide-zinc-200 dark:divide-zinc-800">
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">SelectedTabName</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>string</code> – The name of the tab that should be initially selected. If empty, the first tab is selected.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">string</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">string.Empty</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">TabTextColor</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>SemanticColor</code> – The text color for non-selected tabs.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">SemanticColor</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">SemanticColor.Foreground</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">SelectedTabTextColor</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>SemanticColor</code> – The text color for the selected tab.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">SemanticColor</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">SemanticColor.Primary</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">SelectedTabUnderlineColor</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>SemanticColor</code> – The color for the underline marker of the selected tab.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">SemanticColor</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">SemanticColor.Primary</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">TabBackgroundColor</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>SemanticColor</code> – The background color for the tabs (applied to individual tabs).</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">SemanticColor</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">SemanticColor.Background</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">Menu</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>RenderFragment</code> – The required RenderFragment containing the tab menu structure (usually includes <code class="rounded-md bg-zinc-100 px-1.5 py-0.5 font-mono text-sm font-semibold text-zinc-700 dark:bg-zinc-800 dark:text-zinc-200">RzTabStrip</code>).</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">RenderFragment</code></td>
                                <td class="px-4 py-4"><span class="inline-flex items-center rounded-md bg-green-50 px-2 py-1 text-xs font-medium text-green-700 ring-1 ring-inset ring-green-600/20 dark:bg-green-500/10 dark:text-green-400 dark:ring-green-500/20">Required</span></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">TabPanels</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>RenderFragment</code> – The required RenderFragment containing the <code class="rounded-md bg-zinc-100 px-1.5 py-0.5 font-mono text-sm font-semibold text-zinc-700 dark:bg-zinc-800 dark:text-zinc-200">RzTabPanel</code> components.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">RenderFragment</code></td>
                                <td class="px-4 py-4"><span class="inline-flex items-center rounded-md bg-green-50 px-2 py-1 text-xs font-medium text-green-700 ring-1 ring-inset ring-green-600/20 dark:bg-green-500/10 dark:text-green-400 dark:ring-green-500/20">Required</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>

            <!-- RzTabStrip Component Parameters -->
            <section class="my-8 py-2">
                <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="RzTabStrip Parameters" class="scroll-mt-20">
                    RzTabStrip Component Parameters
                </RzHeading>
                  <div class="not-prose mt-6 mb-10 overflow-hidden rounded-md border border-zinc-200 dark:border-zinc-800">
                    <table class="w-full text-sm">
                        <thead class="text-left bg-zinc-100/75 font-semibold text-zinc-700 dark:bg-zinc-900/50 dark:text-zinc-300">
                            <tr>
                                <th scope="col" class="w-1/5 px-4 py-3">Property</th>
                                <th scope="col" class="w-2/5 px-4 py-3">Description</th>
                                <th scope="col" class="w-1/5 px-4 py-3">Type</th>
                                <th scope="col" class="px-4 py-3">Default</th>
                            </tr>
                        </thead>
                        <tbody class="divide-y divide-zinc-200 dark:divide-zinc-800">
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">Justify</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>Justify</code> – Horizontal alignment of tab content within the strip.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">Justify</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">Justify.Center</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">SpaceBetween</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>Size</code> – Gap spacing between tabs.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">Size</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">Size.Medium</code></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">ChildContent</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>RenderFragment?</code> – The child content, expected to be <code class="rounded-md bg-zinc-100 px-1.5 py-0.5 font-mono text-sm font-semibold text-zinc-700 dark:bg-zinc-800 dark:text-zinc-200">RzTab</code> components.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">RenderFragment?</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">null</code></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>

            <!-- RzTab Component Parameters -->
            <section class="my-8 py-2">
                <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="RzTab Parameters" class="scroll-mt-20">
                    RzTab Component Parameters
                </RzHeading>
                  <div class="not-prose mt-6 mb-10 overflow-hidden rounded-md border border-zinc-200 dark:border-zinc-800">
                    <table class="w-full text-sm">
                        <thead class="text-left bg-zinc-100/75 font-semibold text-zinc-700 dark:bg-zinc-900/50 dark:text-zinc-300">
                            <tr>
                                <th scope="col" class="w-1/5 px-4 py-3">Property</th>
                                <th scope="col" class="w-2/5 px-4 py-3">Description</th>
                                <th scope="col" class="w-1/5 px-4 py-3">Type</th>
                                <th scope="col" class="px-4 py-3">Default</th>
                            </tr>
                        </thead>
                        <tbody class="divide-y divide-zinc-200 dark:divide-zinc-800">
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">Name</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>string</code> – The unique name identifier for this tab. Must match the Name of the corresponding <code class="rounded-md bg-zinc-100 px-1.5 py-0.5 font-mono text-sm font-semibold text-zinc-700 dark:bg-zinc-800 dark:text-zinc-200">RzTabPanel</code>.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">string</code></td>
                                <td class="px-4 py-4"><span class="inline-flex items-center rounded-md bg-green-50 px-2 py-1 text-xs font-medium text-green-700 ring-1 ring-inset ring-green-600/20 dark:bg-green-500/10 dark:text-green-400 dark:ring-green-500/20">Required</span></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">ChildContent</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>RenderFragment?</code> – The content to be displayed inside the tab button (e.g., text, icon).</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">RenderFragment?</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">null</code></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>

            <!-- RzTabPanel Component Parameters -->
            <section class="my-8 py-2">
                <RzHeading Level="HeadingLevel.H2" QuickReferenceTitle="RzTabPanel Parameters" class="scroll-mt-20">
                    RzTabPanel Component Parameters
                </RzHeading>
                  <div class="not-prose mt-6 mb-10 overflow-hidden rounded-md border border-zinc-200 dark:border-zinc-800">
                    <table class="w-full text-sm">
                        <thead class="text-left bg-zinc-100/75 font-semibold text-zinc-700 dark:bg-zinc-900/50 dark:text-zinc-300">
                            <tr>
                                <th scope="col" class="w-1/5 px-4 py-3">Property</th>
                                <th scope="col" class="w-2/5 px-4 py-3">Description</th>
                                <th scope="col" class="w-1/5 px-4 py-3">Type</th>
                                <th scope="col" class="px-4 py-3">Default</th>
                            </tr>
                        </thead>
                        <tbody class="divide-y divide-zinc-200 dark:divide-zinc-800">
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">Name</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>string</code> – The unique name identifier for this panel. Must match the Name of the corresponding <code class="rounded-md bg-zinc-100 px-1.5 py-0.5 font-mono text-sm font-semibold text-zinc-700 dark:bg-zinc-800 dark:text-zinc-200">RzTab</code>.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">string</code></td>
                                <td class="px-4 py-4"><span class="inline-flex items-center rounded-md bg-green-50 px-2 py-1 text-xs font-medium text-green-700 ring-1 ring-inset ring-green-600/20 dark:bg-green-500/10 dark:text-green-400 dark:ring-green-500/20">Required</span></td>
                            </tr>
                            <tr class="align-top">
                                <td class="px-4 py-4"><code class="font-mono font-medium text-sky-600 dark:text-sky-400">ChildContent</code></td>
                                <td class="px-4 py-4 text-zinc-600 dark:text-zinc-400"><code>RenderFragment?</code> – The content to be displayed inside the tab panel when active.</td>
                                <td class="px-4 py-4"><code class="font-mono text-violet-600 dark:text-violet-400">RenderFragment?</code></td>
                                <td class="px-4 py-4 text-zinc-500"><code class="font-mono">null</code></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </section>
        </MainContent>
    </RzArticle>
</RzQuickReferenceContainer>